help = setup MySql access info
abbr = conf
cmd = config.bash

[args]
host|h|H = 127.0.0.1
port|p|P = 4000
user|u|U = root
verify-access|verify|v|V = true

[arg2env]
mysql.host = host
mysql.port = port
mysql.user = user

[env]
mysql.host = write
mysql.port = write
mysql.user = write

[dep]
mysql = as client
